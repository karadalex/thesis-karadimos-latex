@Article{Rosa2015,
  author={Rosa, Beno{\^i}t
  and Gruijthuijsen, Caspar
  and Van Cleynenbreugel, Ben
  and Sloten, Jos Vander
  and Reynaerts, Dominiek
  and Poorten, Emmanuel Vander},
  title={Estimation of optimal pivot point for remote center of motion alignment in surgery},
  journal={International Journal of Computer Assisted Radiology and Surgery},
  year={2015},
  month={02},
  day={01},
  volume={10},
  number={2},
  pages={205-215},
  abstract={   The determination of an optimal pivot point ({\$}{\$}{\{} OPP{\}}{\$}{\$}OPP) is important for instrument manipulation in minimally invasive surgery. Such knowledge is of particular importance for robotic-assisted surgery where robots need to rotate precisely around a specific point in space in order to minimize trauma to the body wall while maintaining position control. Remote center of motion (RCM) mechanisms are commonly used, where the RCM point is manually and visually aligned. If not positioned appropriately, this misalignment might lead to intolerably high forces on the body wall with increased risk of postoperative complications or instrument damage. An automated method to align the RCM with the {\$}{\$}{\{} OPP{\}}{\$}{\$}OPPwas developed and tested.},
  issn={1861-6429},
  doi={10.1007/s11548-014-1071-3},
  url={https://doi.org/10.1007/s11548-014-1071-3}
}

